            result = JSON.parse(result);

            if (result['success_msg'] != '') {
              $(cur_id).parent().find('.w-form-done').html('<div>' + result['success_msg'] + '</div>');
            }

            $(cur_id).parent().find('.w-form-fail').html('<div>' + result['error_msg'] + '</div>');

